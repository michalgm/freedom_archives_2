{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true

    // "paths": {
    //   "@mui/icons-material/*": ["../node_modules/@mui/icons-material/esm/*"]
    // }
  },
  "include": [
    "src/**/*",
    "vite-env.d.ts",
    "vite.config.ts",
    "react-router.config.ts",
    ".storybook/**/*",
    "../backend/schema/schemaUtils.js"
  ],
  "exclude": ["node_modules", "dist", "build", "coverage", "storybook-static", "public", ".react-router"]
}
