{
  "name": "freedom-archives-database",
  "description": "Freedom Archives database backend",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "backend",
  "keywords": [
    "feathers"
  ],
  "workspaces": [
    "frontend"
  ],
  "author": {
    "name": "Greg",
    "email": "greg@primate.net"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 20.18.1"
  },
  "feathers": {
    "language": "js",
    "packager": "npm",
    "database": "postgresql",
    "framework": "express",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "type": "module",
  "directories": {
    "lib": "backend",
    "test": "test/",
    "config": "config/"
  },
  "scripts": {
    "eslint": "yarn eslint backend/. test/. --config .eslintrc.json",
    "dev": "LD_PRELOAD=${LD_PRELOAD:-}/usr/lib/x86_64-linux-gnu/libjemalloc.so yarn nodemon -d 1 -w config/ -w backend/ -w package.json backend/index.js",
    "start": "yarn concurrently -k \"yarn run dev\" \"cd ./frontend; yarn run start\"",
    "start:prod": "node backend/",
    "mocha": "NODE_ENV=test mocha test/ --ignore '**/helpers/**' --recursive --parallel --exit",
    "test": "NODE_ENV=test yarn run migrate && yarn run mocha",
    "bundle:client": "yarn pack --pack-destination ./public",
    "migrate": "yarn knex migrate:latest",
    "migrate:prod": "NODE_ENV=production yarn knex migrate:latest",
    "migrate:make": "yarn knex migrate:make -x mjs"
  },
  "dependencies": {
    "@babel/core": "^7.28.5",
    "@feathersjs/authentication": "^5.0.35",
    "@feathersjs/authentication-local": "^5.0.35",
    "@feathersjs/authentication-oauth": "^5.0.35",
    "@feathersjs/cli": "^5.0.35",
    "@feathersjs/configuration": "^5.0.35",
    "@feathersjs/errors": "^5.0.35",
    "@feathersjs/express": "^5.0.35",
    "@feathersjs/feathers": "^5.0.35",
    "@feathersjs/knex": "^5.0.35",
    "@feathersjs/schema": "^5.0.35",
    "@feathersjs/typebox": "^5.0.35",
    "@img/sharp-libvips-linux-x64": "https://raw.githubusercontent.com/michalgm/freedom_archives_2/master/sharp-libvips/img-sharp-libvips-linux-x64-1.1.0.tgz",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "helmet": "^8.1.0",
    "knex": "^3.1.0",
    "node-addon-api": "^8.5.0",
    "node-gyp": "^11.5.0",
    "pg": "^8.16.3",
    "pg-tsquery": "^8.4.2",
    "serve-favicon": "^2.5.1",
    "sharp": "0.34.2",
    "string-strip-html": "^13.5.0",
    "typescript": "^5.9.3",
    "winston": "^3.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.0",
    "@eslint/json": "^0.13.2",
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "axios": "^1.13.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.0",
    "globals": "^16.4.0",
    "mocha": "^11.7.4",
    "mock-knex": "^0.4.13",
    "nodemon": "^3.1.10",
    "sinon": "^19.0.5",
    "typescript-eslint": "^8.46.2"
  },
  "msw": {
    "workerDirectory": [
      "frontend/public"
    ]
  },
  "overrides": {
    "@img/sharp-libvips-linux-x64": "https://raw.githubusercontent.com/michalgm/freedom_archives_2/master/sharp-libvips/img-sharp-libvips-linux-x64-1.1.0.tgz"
  },
  "packageManager": "yarn@4.10.3+sha512.c38cafb5c7bb273f3926d04e55e1d8c9dfa7d9c3ea1f36a4868fa028b9e5f72298f0b7f401ad5eb921749eb012eb1c3bb74bf7503df3ee43fd600d14a018266f"
}