    "@feathersjs/errors": "^4.5.11",
    "@feathersjs/express": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-knex": "git+https://github.com/michalgm/feathers-knex.git#add-overlap-operator",
    "helmet": "^4.6.0",
    "knex": "^0.21.19",
    "pg": "^8.6.0",
    "pg-tsquery": "^8.2.0",
    "serve-favicon": "^2.5.0",
