  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "backend",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^10.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint backend/. test/. --config .eslintrc.json",
    "dev": "nodemon -d 1 -w config/ -w backend/ -w package.json backend/index.js",
