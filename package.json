{
  "name": "freedom-archives-database",
  "description": "Freedom Archives database backend",
  "version": "0.0.8",
  "homepage": "",
  "private": true,
  "main": "backend",
  "keywords": [
    "feathers"
  ],
  "workspaces": [
    "frontend"
  ],
  "author": {
    "name": "Greg",
    "email": "greg@primate.net"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 20.18.1"
  },
  "feathers": {
    "language": "js",
    "packager": "npm",
    "database": "postgresql",
    "framework": "express",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "type": "module",
  "directories": {
    "lib": "backend",
    "test": "test/",
    "config": "config/"
  },
  "scripts": {
    "eslint": "yarn eslint backend/. test/. --config .eslintrc.json",
    "dev": "LD_PRELOAD=${LD_PRELOAD:-}/usr/lib/x86_64-linux-gnu/libjemalloc.so yarn nodemon -d 1 -w config/ -w backend/ -w package.json backend/index.js",
    "start": "yarn concurrently -k \"yarn run dev\" \"cd ./frontend; yarn run start\"",
    "start:prod": "node backend/",
    "test": "NODE_ENV=test vitest run",
    "test:watch": "NODE_ENV=test vitest",
    "bundle:client": "yarn pack --pack-destination ./public",
    "migrate": "yarn knex migrate:latest",
    "migrate:prod": "NODE_ENV=production yarn knex migrate:latest",
    "migrate:make": "yarn knex migrate:make -x mjs",
    "release": "yarn version $0 && git tag -a v$(jq -r '.version' package.json) -m \"Release version v$(jq -r '.version' package.json)\" && git push --tags"
  },
  "dependencies": {
    "@babel/core": "^7.28.6",
    "@feathersjs/authentication": "^5.0.37",
    "@feathersjs/authentication-local": "^5.0.37",
    "@feathersjs/authentication-oauth": "^5.0.37",
    "@feathersjs/cli": "^5.0.37",
    "@feathersjs/configuration": "^5.0.37",
    "@feathersjs/errors": "^5.0.37",
    "@feathersjs/express": "^5.0.37",
    "@feathersjs/feathers": "^5.0.37",
    "@feathersjs/knex": "^5.0.37",
    "@feathersjs/schema": "^5.0.37",
    "@feathersjs/typebox": "^5.0.37",
    "@img/sharp-libvips-linux-x64": "https://raw.githubusercontent.com/michalgm/freedom_archives_2/master/sharp-libvips/img-sharp-libvips-linux-x64-1.1.0.tgz",
    "@react-router/express": "^7.12.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "helmet": "^8.1.0",
    "knex": "^3.1.0",
    "node-addon-api": "^8.5.0",
    "pg": "^8.16.3",
    "pg-tsquery": "^8.4.2",
    "serve-favicon": "^2.5.1",
    "sharp": "0.34.2",
    "sitemap": "^9.0.0",
    "string-strip-html": "^13.5.0",
    "typescript": "^5.9.3",
    "winston": "^3.19.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.6",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@stylistic/eslint-plugin": "^5.7.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/ui": "^4.0.17",
    "axios": "^1.13.2",
    "baseline-browser-mapping": "^2.9.14",
    "browserslist": "^4.28.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "mock-knex": "^0.4.13",
    "nodemon": "^3.1.11",
    "sinon": "^19.0.5",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.17"
  },
  "msw": {
    "workerDirectory": [
      "frontend/public"
    ]
  },
  "overrides": {
    "@img/sharp-libvips-linux-x64": "https://raw.githubusercontent.com/michalgm/freedom_archives_2/master/sharp-libvips/img-sharp-libvips-linux-x64-1.1.0.tgz"
  },
  "resolutions": {
    "browserslist": "^4.28.1"
  },
  "packageManager": "yarn@4.12.0"
}
